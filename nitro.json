{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": [
    "webrtc"
  ],
  "ios": {
    "iosModuleName": "Webrtc"
  },
  "android": {
    "androidNamespace": [
      "webrtc"
    ],
    "androidCxxLibName": "Webrtc"
  },
  "autolinking": {
    "Permissions": {
      "swift": "HybridPermissions",
      "kotlin": "HybridPermissions"
    },
    "Microphone": {
      "swift": "HybridMicrophone",
      "kotlin": "HybridMicrophone"
    },
    "Camera": {
      "swift": "HybridCamera",
      "kotlin": "HybridCamera"
    },
    "MediaStreamTrack": {
      "cpp": "HybridMediaStreamTrack"
    },
    "MediaStream": {
      "cpp": "HybridMediaStream"
    },
    "WebrtcView": {
      "swift": "HybridWebrtcView",
      "kotlin": "HybridWebrtcView"
    },
    "MediaDevices": {
      "cpp": "HybridMediaDevices"
    },
    "MediaRecorder": {
      "cpp": "HybridMediaRecorder"
    },
    "RTCRtpSender": {
      "cpp": "HybridRTCRtpSender"
    },
    "RTCRtpReceiver": {
      "cpp": "HybridRTCRtpReceiver"
    },
    "RTCRtpTransceiver": {
      "cpp": "HybridRTCRtpTransceiver"
    },
    "RTCPeerConnection": {
      "cpp": "HybridRTCPeerConnection"
    }
  },
  "ignorePaths": [
    "**/node_modules"
  ]
}